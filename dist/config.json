{
  "name": "Google Agenda Events",
  "description": "Displays coming events from a Google Agenda",
  "frontend": {
    "eltName": "marcel-plugin-calendar",
    "cols": 4,
    "rows": 1,
    "fixedsized": 0,
    "props": [
      {
        "name": "url",
        "description": "Url of the server that send the events",
        "type": "string",
        "value": "http://localhost:5000"
      }
    ]
  },
  "backend": {
    "ports": [
      8080
    ],
    "props": {
      "marcel_agenda_id" : "id_of_your_google_agenda",
      "google_api_key_file": "your_google_api_key_file"
    },
    "externalfiles" : [

    ]
  }
}